{
    "name": "siilike/cachetool",
    "description": "Manage your OPcache & APCu cache through the CLI",
    "keywords": [
        "cache",
        "opcache",
        "apcu",
        "fastcgi",
        "opcode",
        "fpm"
    ],
    "require": {
        "hollodotme/fast-cgi-client": "^3",
        "php": ">=8.2",
        "psr/log": "^2 || ^3",
        "monolog/monolog": "^3",
        "symfony/finder": "^7",
        "symfony/process": "^7",
        "symfony/http-client": "^7",
        "symfony/http-foundation": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "gordalina",
            "email": "samuel.gordalina@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CacheTool\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CacheTool\\": "tests/"
        }
    }
}
